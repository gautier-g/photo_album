<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.media.MediaView?>

<VBox alignment="CENTER" maxWidth="400.0" minWidth="400.0" prefHeight="200.0" prefWidth="200.0" fx:controller="album.vues.VuePages" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="ajouterPhotosTitre" alignment="CENTER" contentDisplay="CENTER" onMouseClicked="#ajouterPhotos" text="Ajouter des photos">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </padding></Label>
        <MediaView fitHeight="200.0" fitWidth="200.0">
            <VBox.margin>
                <Insets bottom="50.0" left="50.0" right="50.0" top="50.0" />
            </VBox.margin></MediaView>
    </children>
</VBox>
